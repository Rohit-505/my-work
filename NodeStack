package NonLinear;

public class NodeStack {

	node stck[]=new node[20];
	int top=-1;
	public void push(node n) {
		if(top==19) {
			System.out.println("stack overflow");
			
		}
		else {
		top++;
		stck[top]=n;}
	}
	public void pop() {
		if(top==-1) {
			System.out.println("stack empty");}
		else {
			top--;
		}
	}
	public node top(){
		return stck[top];
	}
		
}

